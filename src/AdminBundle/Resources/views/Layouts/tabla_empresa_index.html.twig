{% for lempresa in lista_empresa %}
	<tr>
		<td>{{ lempresa.rut }}</td>
		<td>{{ lempresa.nombre }}</td>
		<td>{{ lempresa.email }}</td>
		<td>{{ lempresa.telefono }}</td>
		<td>
			<a class="btn_dele_cont cur_poin text-danger" data-id="{{ lempresa.id }}" data-toggle="tooltip" data-placement="top" title="Eliminar"><i class="red-text small material-icons ">cancel</i></a>
			<a class="btn_test cur_poin" href="{{ url('admin_empresa_editar',{id:lempresa.id}) }}" data-toggle="tooltip" data-placement="bottom" title="Editar"><i class="small material-icons ">mode_edit</i></a>
		</td>
	</tr>
{% endfor %}